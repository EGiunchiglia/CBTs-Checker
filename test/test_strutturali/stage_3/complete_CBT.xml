</ActionTemplate>


<root>
    <BehaviorTree>
        <Fallback>
            <Sequence>
                <YARPCondition ID="Condition1"/>
                <YARPAction ID="Action1"/>
            </Sequence>
            <Parallel>
                <YARPAction ID="Action1"/>
                <YARPAction ID="Action2"/>
